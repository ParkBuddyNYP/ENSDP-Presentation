<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallwhite"


    tools:context=".LotLayout.BaseThreeFragment">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"


        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01999998">


        <RadioButton
            android:id="@+id/rbBooked"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:buttonTint="@color/grey"
            android:checked="true"
            android:text="Booked"
            android:textColor="#AE464642"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/rbAvailable"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:buttonTint="@color/green"
            android:checked="true"
            android:text="Available"
            android:textColor="#167212"
            android:textStyle="bold" />

        <RadioButton
            android:id="@+id/rbSelected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:buttonTint="@color/instabug_annotation_color_blue"
            android:checked="true"
            android:text="Selected"
            android:textColor="#3357C9"
            android:textStyle="bold" />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="290dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/dashed_border"
        android:orientation="vertical"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BO1"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb36"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/gcarr" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AO2"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb17"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/grcarr" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BO3"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb38"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/gcarr" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BO4"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb19"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/grcarr" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="BO5"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb30"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/gcarr" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="197dp"
        android:background="@drawable/dashed_border"
        android:orientation="vertical"

        app:layout_constraintEnd_toStartOf="@+id/linearLayout2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AO1"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb31"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/gcarl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="AO2"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb32"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/grcarl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AO3"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb13"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/grcarl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AO4"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb34"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/gcarl" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="AO5"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imgb15"
            android:layout_width="55dp"
            android:layout_height="45dp"
            android:src="@drawable/grcarl" />


    </LinearLayout>

    <Button
        android:id="@+id/buti"
        android:layout_width="330dp"
        android:layout_height="50sp"

        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/buttonstyle7"
        android:text="Proceed with Spot"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.859" />


</android.support.constraint.ConstraintLayout>